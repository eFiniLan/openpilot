+      l = json.loads(last_pos)
+      lat = float(l["latitude"])
+      lon = float(l["longitude"])
+      self.location_rad = np.radians(np.array([lat, lon], dtype=float))
+      self.location_deg = (lat, lon)
+      self.bearing_rad = np.radians(0, dtype=float)
