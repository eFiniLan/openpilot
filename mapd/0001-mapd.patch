+      slc.map_speed_limit = 0
+      slc.write_map_state()
+      # }} PFEIFER - SLC
+      return
+
+    # PFEIFER - SLC {{
+    slc.load_state()
+    slc.map_speed_limit = self.route.current_speed_limit
+    slc.write_map_state()
+    # }} PFEIFER - SLC
+
